{
  "name": "expressApp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js",
    "build-style": "stylus public/stylesheets/stylus/*.styl --out public/stylesheets/css",
    "watch": "watch 'npm run build-style' ./public/stylesheets/stylus",
    "pwd": "env",
    "deploy": "forever stopall && npm install && forever start app.js"
  },
  "dependencies": {
    "bcrypt": "~0.8.1",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "express-session": "~1.10.4",
    "jade": "~1.6.0",
    "monk": "~1.0.0",
    "morgan": "~1.3.0",
    "multer": "~0.1.8",
    "passport": "~0.2.1",
    "passport-local": "~1.0.0",
    "request": "~2.53.0",
    "serve-favicon": "~2.1.3",
    "webdriverio": "^3.4.0"
  }
}
